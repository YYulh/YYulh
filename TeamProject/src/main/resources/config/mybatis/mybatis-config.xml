<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN" "HTTP://mybatis.org/dtd/mybatis-3-config.dtd">

<configuration>
	<settings>
		<setting name="cacheEnabled" value="false" />
		<setting name="useGeneratedKeys" value="true" />
		<setting name="defaultExecutorType" value="REUSE" />
	</settings>
	<typeAliases>

<typeAlias alias="member" type="vo.MemberVO"/>

<typeAlias alias="product" type="vo.ProductVO"/>

<typeAlias alias="grade" type="vo.GradeVO"/>

<typeAlias alias="coupon" type="vo.CouponVO"/>

<typeAlias alias="order" type="vo.OrderVO"/>

</typeAliases>
	<mappers>	
		<mapper resource="config/mybatis/mapper/home.xml"/>	
		<mapper resource="config/mybatis/mapper/member.xml"/>
		<mapper resource="config/mybatis/mapper/product.xml"/>
		<mapper resource="config/mybatis/mapper/login.xml"/>
		<mapper resource="config/mybatis/mapper/mypage.xml"/>
		<mapper resource="config/mybatis/mapper/stock.xml"/>
		<mapper resource="config/mybatis/mapper/productlist.xml"/>
		<mapper resource="config/mybatis/mapper/management.xml"/>
		<mapper resource="config/mybatis/mapper/order.xml"/>
		<mapper resource="config/mybatis/mapper/board.xml"/>
		<mapper resource="config/mybatis/mapper/coupon.xml"/>
		<mapper resource="config/mybatis/mapper/categorie.xml"/>
		<mapper resource="config/mybatis/mapper/grade.xml"/>
		<mapper resource="config/mybatis/mapper/join.xml"/>
	</mappers>
</configuration>